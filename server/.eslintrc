{
    "env": {
        "browser": true,
        "commonjs": false,
        "es6": true
    },
    "extends": ["airbnb-base"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "strict": 0,
        "arrow-body-style": 0,
        "no-unused-expressions": [
            "error",
            {
                "allowTernary": true
            }
        ],
        "no-console": 0,
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "never"],
        "spaced-comment": 0,
        "max-len": 0,
        "camelcase": 0,
        "comma-dangle": ["error", "never"],
        "object-curly-spacing": 0,
        "no-nested-ternary": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0,
        "import/prefer-default-export": "off",
        "radix": "off",
        "no-unused-vars": "off",
        "no-restricted-syntax": 0,
        "no-await-in-loop": 0,
        "no-useless-escape": 0,
        "consistent-return": 0,
        "eqeqeq": 0
    }
}
